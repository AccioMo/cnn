{
	"input": {
		"width": 28,
		"height": 28,
		"channels": 1
	},
	"conv_layers": [
		{
			"filters": 12,
			"kernel_size": 5,
			"stride": 1,
			"padding": "same",
			"activation": "relu"
		},
		{
			"filters": 15,
			"kernel_size": 3,
			"stride": 1,
			"padding": "same",
			"activation": "relu"
		},
		{
			"filters": 20,
			"kernel_size": 1,
			"stride": 1,
			"padding": "same",
			"activation": "relu"
		}
	],
	"hidden_layers": [
		{
			"units": 128,
			"activation": "relu"
