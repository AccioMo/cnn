{
	"input": {
		"width": 28,
		"height": 28,
		"channels": 1
	},
	"conv_layers": [
		{
			"filters": 16,
			"kernel_size": 3,
			"stride": 1,
			"padding": "same",
			"activation": "relu",
			"pooling": {"type": "max", "size": 2}
		},
		{
			"filters": 32,
			"kernel_size": 3,
			"stride": 1,
			"padding": "same",
			"activation": "relu",
			"pooling": {"type": "max", "size": 2}
		}
	],
	"hidden_layers": [
		{
			"units": 128,
			"activation": "relu"
		}
	],
	"output_layer": {
		"units": 10,
		"activation": "softmax"
	},
	"epochs": 10,
	"batch_size": 128,
	"iterations": 20,
	"learning_rate": 0.001,
	"l2_reg": 0.0001
}
